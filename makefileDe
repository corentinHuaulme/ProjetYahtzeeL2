prog : de.o combinaison.o feuilleScore.o joueur.o main.o
	gcc main.o de.o combinaison.o joueur.o feuilleScore.o -o prog

mainDe.o : main.c
	gcc -c main.c -o main.o

joueur.o : joueur.c 
	gcc -c joueur.c -o joueur.o
	
	
de.o : de.c
	gcc -c de.c -o de.o

combinaison.o : combinaison.c
	gcc -c combinaison.c -o combinaison.o

feuilleScore.o : feuilleScore.c
	gcc -c feuilleScore.c -o feuilleScore.o

